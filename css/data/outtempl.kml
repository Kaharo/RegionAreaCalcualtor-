<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>Should be named as XML</name>
	<Style id="blueFootprintOutline">
		<IconStyle>
			<scale>1.3</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LineStyle>
			<color>ffff0000</color>
		</LineStyle>
		<PolyStyle>
			<color>00000000</color>
		</PolyStyle>
	</Style>
	<Folder>
		<name>Shape</name>
		<Placemark>
			<name>Information</name>
			<description>come from js CDATA</description>
			<styleUrl>#blueFootprintOutline</styleUrl>
			<Polygon>
				<tessellate>1</tessellate>
				<gx:drawOrder>1</gx:drawOrder>
				<outerBoundaryIs>
					<LinearRing>
						<coordinates>
							lon,lat,0  lon,lat,0 ...
						</coordinates>
					</LinearRing>
				</outerBoundaryIs>
			</Polygon>
		</Placemark>
	</Folder>
	<Folder>
		<name>Should be named as XML</name>
		<GroundOverlay>
			<name>Info</name>
			<styleUrl>#blueFootprintOutline</styleUrl>
			<Icon>
				<href>Should be named as XML.png</href>
				<viewBoundScale>0.75</viewBoundScale>
			</Icon>
			<gx:LatLonQuad>
				<coordinates>
					lon,lat,0  lon,lat,0 ...
				</coordinates>
				<tessellate>1</tessellate>
			</gx:LatLonQuad>
		</GroundOverlay>
	</Folder>
</Document>
</kml>
